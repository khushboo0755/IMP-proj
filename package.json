{
  "name": "sbh-webchat-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "./node_modules/.bin/ng serve -c local --deploy-url=http://localhost:3440/",
    "build-draft-local": "./node_modules/.bin/ng build --deploy-url=http://localhost:4200/assets/minifiedFiles/ -c draft-local && npm run package-es2015",
    "build-draft-dev": "./node_modules/.bin/ng build --deploy-url=https://engg-dev-core.smartbothub.com:3443/assets/minifiedFiles/ -c draft-dev && npm run package-es2015",
    "build-draft-preprod": "./node_modules/.bin/ng build --deploy-url=https://engg-preprod-core.smartbothub.com:3443/assets/minifiedFiles/ -c draft-preprod && npm run package-es2015",
    "build-draft-test": "./node_modules/.bin/ng build --deploy-url=https://engg-test-core.smartbothub.com:3443/assets/minifiedFiles/ -c draft-test && npm run package-es2015",
    "build-draft-beta": "./node_modules/.bin/ng build --deploy-url=https://beta.smartbothub.com:3443/assets/minifiedFiles/ -c draft-beta && npm run package-es2015",
    "package-es2015": "jscat ./dist/SBH-Webchat-UI/runtime-es2015.js ./dist/SBH-Webchat-UI/polyfills-es2015.js ./dist/SBH-Webchat-UI/scripts.js ./dist/SBH-Webchat-UI/main-es2015.js ./dist/SBH-Webchat-UI/vendor-es2015.js ./dist/SBH-Webchat-UI/common-es2015.js > ./dist/SBH-Webchat-UI/SBH-Webchat-UI.js",
    "package-es5": "jscat ./dist/SBH-Webchat-UI/runtime-es5.js ./dist/SBH-Webchat-UI/polyfills-es5.js ./dist/SBH-Webchat-UI/scripts.js ./dist/SBH-Webchat-UI/main-es5.js ./dist/SBH-Webchat-UI/vendor-es5.js ./dist/SBH-Webchat-UI/common-es5.js > ./dist/SBH-Webchat-UI/SBH-Webchat-UI-es5.js",
    "build-local": "./node_modules/.bin/ng build --deploy-url=http://localhost:3440/ -c local",
    "build-dev": "./node_modules/.bin/ng build  --deploy-url=https://engg-dev-core.smartbothub.com/3440/ -c dev",
    "build-preprod": "./node_modules/.bin/ng build --deploy-url=https://engg-preprod-core.smartbothub.com/3440/ -c preprod",
    "build-test": "./node_modules/.bin/ng build --deploy-url=https://engg-test-core.smartbothub.com/3440/ -c test",
    "build-beta": "./node_modules/.bin/ng build --deploy-url=https://beta.smartbothub.com/3440/ -c beta",
    "test": "ng test",
    "lint": "ng lint",
    "docs": "npx docco src/**/**/**/**/**/**.ts",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/elements": "^12.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@webcomponents/custom-elements": "^1.5.0",
    "bootstrap": "^4.6.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.14.0",
    "docco": "^0.8.1",
    "jscat": "0.0.2",
    "leaflet": "^1.7.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.16.0",
    "ngx-file-drop": "^11.1.0",
    "ngx-logger": "^4.2.1",
    "rxjs": "~6.5.4",
    "swiper": "^6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^8.3.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
